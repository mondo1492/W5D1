<title>Edit Sub</title>

<form action="<%= sub_url(@sub) %>" method="post">
  <input type="hidden" name="_method" value="patch">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <label>Title
    <input type="text" name="sub[title]" value="">
  </label>
  <label>Description
    <input type="text" name="sub[description]" value="">
  </label>
  <button type="submit" value="Edit Sub"></button>
</form>
